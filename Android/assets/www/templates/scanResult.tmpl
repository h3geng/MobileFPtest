<div class="equipmentscannedcontainer" data-role="collapsible-set" data-theme="b" data-content-theme="d">
    <div class="equipmentscannedcontainerdata" data-role="collapsible" data-collapsed="false">
        <h2>Equipment Scanned</h2>
        <ul class="equipmentscanned" data-role="listview" data-theme="d" data-divider-theme="d">
            <li id="${id}">
                <a href="#equipmentDetails">
                    <h3>${itemClass} - ${itemModel}</h3>
                    <p>Item: <strong>${itemNumber}</strong> Tag: <strong>${assetTag}</strong></p>
                    <p>Status: <strong>${status} [${branch}]</strong></p>
                </a>
            </li>
        </ul>
    </div>
</div>